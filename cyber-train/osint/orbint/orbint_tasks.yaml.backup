rfi_launch_analysis:
  description: >
    Investigate rocket launch and attribute payload.
  trigger: on_demand
  agent: launch_intelligence_officer
  inputs:
    launch_id: str
  tools:
  - SatNOGS_oss
  - HeavensAbove_oss
  - LeoLabs
  - ExoAnalytic
  - Maxar SecureWatch
  expected_output: >
    {"launch_id":"XYZ123","payload":"military comsat","attribution":"Nation A"}
  outputs:
    report_uri: uri
    attribution: str
  governance:
    legal_basis: open launch feeds
    chain_of_custody: s3://orbint/coc/{launch_id}.zip
  sla:
    turnaround_hours: 6
  async_execution: false
  human_input: false
  markdown: true
  create_directory: true
  guardrail_max_retries: 3
rfi_asat_event_investigation:
  description: >
    Investigate suspected ASAT test or co-orbital hostile maneuver.
  trigger: on_demand
  agent: adversarial_space_ops_analyst
  inputs:
    event_id: str
  tools:
  - CelesTrak_oss
  - Skyfield_oss
  - ExoAnalytic
  - HawkEye360
  expected_output: >
    {"event_id":"ASAT2025","assessment":"co-orbital threat","confidence":0.91}
  outputs:
    report_uri: uri
    confidence_score: float
  governance:
    legal_basis: open orbital catalogs + commercial SSA
    chain_of_custody: s3://orbint/coc/{event_id}.zip
  sla:
    turnaround_hours: 12
  async_execution: false
  human_input: false
  markdown: true
  create_directory: true
  guardrail_max_retries: 3
rfi_megaconstellation_outage:
  description: >
    Investigate outage or degraded performance in a mega-constellation (Starlink, OneWeb).
  trigger: on_demand
  agent: megaconstellation_analyst
  inputs:
    constellation: str
    time_window:
    - start
    - end
  tools:
  - Skyfield_oss
  - OrbitPy_oss
  - LeoLabs
  - Slingshot Aerospace
  expected_output: >
    {"constellation":"Starlink","outage":"regional","duration":"2h"}
  outputs:
    outage_report: uri
  sla:
    turnaround_hours: 8
  async_execution: false
  human_input: false
  markdown: true
  create_directory: true
  guardrail_max_retries: 3
