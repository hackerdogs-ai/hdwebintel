rfi_sanctions_evasion_case:
  description: >
    Investigate suspected sanctions evasion involving a shipment or entity.
  trigger: on_demand
  agent: sanctions_evasion_specialist
  inputs:
    entity_name: str
    shipment_id: str
  tools:
  - OpenSanctions
  - Orbis_BvD
  - ImportGenius
  - Refinitiv
  expected_output: >
    {"entity":"XYZ Ltd","shipment":"ID123","finding":"linked to sanctioned entity","confidence_score":0.91,"risk_level":"high"}
  outputs:
    report_uri: str
  governance:
    legal_basis: AML/CTF compliance
    chain_of_custody: s3://tradint/coc/{shipment_id}.zip
  sla:
    turnaround_hours: 12
  async_execution: false
  human_input: false
  markdown: true
  create_directory: true
  guardrail_max_retries: 3
