rfi_adtech_tracker_investigation:
  description: >
    Investigate suspected malicious or privacy-violating AdTech trackers in mobile or web telemetry flows.
  trigger: on_demand
  agent: adtech_tracker_specialist
  inputs:
    app_or_domain: str
    tenant_id: str
  tools:
  - TrackerRadar
  - AppsFlyer
  - Adjust
  - DisconnectDB
  expected_output: "{\"trackers_detected\":[\"trackerX\"],\"risk\":\"high\",\"confidence_score\"\
    :0.92,\n \"recommendations\":[\"remove trackerX\"]}\n"
  outputs:
    adtech_investigation_report: str
  sla:
    turnaround_hours: 12
  async_execution: false
  human_input: false
  markdown: true
  create_directory: true
  guardrail_max_retries: 3
rfi_connected_vehicle_analysis:
  description: >
    Analyze telemetry from connected vehicles for suspicious routing, data leaks, or unauthorized access.
  trigger: on_demand
  agent: automotive_telemetry_engineer
  inputs:
    vehicle_id: str
    time_window_utc:
    - start
    - end
  tools:
  - CANalyzer
  - CarShark
  - UDS_Scanner
  expected_output: "{\"vehicle_id\":\"XYZ123\",\"anomalies\":[\"GPS spoofing\"],\n\
    \ \"confidence_score\":0.9,\"risk_level\":\"critical\"}\n"
  outputs:
    vehicle_analysis_report: str
  sla:
    turnaround_hours: 24
  async_execution: false
  human_input: false
  markdown: true
  create_directory: true
  guardrail_max_retries: 3
