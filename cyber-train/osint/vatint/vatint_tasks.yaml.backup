rfi_satellite_transport_validation:
  description: >
    Validate AIS/ADS-B anomalies using commercial satellite imagery (SAR/EO).
  trigger: on_demand
  agent: satellite_transport_specialist
  inputs:
    anomaly_id: str
    aoi_polygon: geojson
    time_window:
    - start
    - end
    tenant_id: str
  tools:
  - SentinelSat_oss
  - PlanetLabs
  - Maxar SecureWatch
  - Airbus OneAtlas
  - HawkEye360
  expected_output: >
    Annotated imagery and confidence score confirming or rejecting anomaly.
  outputs:
    annotated_imagery: uri
    validation_report: json
    confidence_score: float
  governance:
    legal_basis: commercial imagery license
    privacy_minimization: no PII
    chain_of_custody: s3://vatint/coc/{anomaly_id}.zip
  sla:
    turnaround_hours: 8
  success_metrics:
  - validation_accuracy >= 0.9
  async_execution: false
  human_input: false
  markdown: true
  create_directory: true
  guardrail_max_retries: 3
rfi_signal_spoofing_case:
  description: >
    Investigate suspected GPS, AIS, or ADS-B spoofing/jamming event.
  trigger: on_demand
  agent: adversarial_signal_analyst
  inputs:
    signal_id: str
    aoi_polygon: geojson|null
    time_window:
    - start
    - end
    tenant_id: str
  tools:
  - GNSS_Jammer_Detector_oss
  - GPS-SDR_OSS
  - HawkEye360
  - Spire GNSS
  - SDR#_oss
  expected_output: >
    Technical report confirming spoofing/jamming, with adversarial model indicators.
  outputs:
    spoofing_report: uri
    evidence_pack: zip
    confidence_score: float
  governance:
    legal_basis: open GNSS/AIS/ADS-B feeds
    privacy_minimization: no user PII collected
    chain_of_custody: s3://vatint/coc/{signal_id}.zip
  sla:
    turnaround_hours: 12
  success_metrics:
  - detection_precision >= 0.9
  async_execution: false
  human_input: false
  markdown: true
  create_directory: true
  guardrail_max_retries: 3
