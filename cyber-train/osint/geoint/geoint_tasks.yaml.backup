rfi_ad_hoc_geolocation:
  description: >
    Perform ad-hoc geolocation of a photo or video, with chronolocation if feasible, using terrain features, metadata, and shadow analysis.
  expected_output: "A JSON object with coordinates, time estimate, and evidence pack\
    \ reference. Example: {\"media_uri\": \"s3://uploads/photo1.jpg\",\n         \
    \ \"coordinates\": [36.45, 37.12],\n          \"confidence\": 0.91,\n        \
    \  \"time_window\": [\"2025-09-12T08:00Z\",\"2025-09-12T10:00Z\"],\n         \
    \ \"evidence_pack\": \"s3://evidence/geoloc_123.zip\"}\n"
  agent: geoint_analyst
  output_file: output/rfi_ad_hoc_geolocation.json
  async_execution: false
  human_input: false
  markdown: true
  create_directory: true
  guardrail_max_retries: 3
